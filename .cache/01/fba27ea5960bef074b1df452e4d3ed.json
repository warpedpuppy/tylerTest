 (action.type) {\n        case SET_USER:\n            return action.value;\n        default:\n            return state;\n    }\n}\n\nconst moviesApp = combineReducers({\n    visibilityFilter,\n    movies,\n    currentFavorites,\n    user,\n    selectedView\n})\n\nexport default moviesApp;"},"lineCount":null}},"error":null,"hash":"96b4154ce71415199fa6dd15c9b45d67","cacheData":{"env":{}}}