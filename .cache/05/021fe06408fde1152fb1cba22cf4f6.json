"line":27,"column":4}},{"generated":{"line":86,"column":0},"source":"components/login-view/login-view.jsx","original":{"line":47,"column":1}}],"sources":{"components/login-view/login-view.jsx":"import React, { useState } from 'react';\nimport Form from 'react-bootstrap/Form';\nimport Button from 'react-bootstrap/Button';\nimport axios from 'axios';\nimport config from '../../config'\n\nexport function LoginView(props) {\n    const [ username, setUsername ] = useState('');\n    const [ password, setPassword ] = useState('');\n\n    const handleSubmit = (e) => {\n        e.preventDefault();\n        axios.post(`${config.APIURL}/login`, {\n            Username: username,\n            Password: password\n        })\n        .then(response => {\n            const data = response.data;\n            console.log(data);\n            props.onLoggedIn(data);\n        })\n        .catch(e => {\n            console.log('no such user')\n        });\n    };\n\n    return (\n        <Form>\n            <Form.Group controlId='formUsername'>\n                <Form.Label>Username:</Form.Label>\n                <Form.Control type='text' placeholder='Enter Username' onChange={e => setUsername(e.target.value)} />\n            </Form.Group>\n\n            <Form.Group controlId='formPassword'>\n                <Form.Label>Password:</Form.Label>\n                <Form.Control type='password' placeholder='Enter Password' onChange={e => setPassword(e.target.value)} />\n            </Form.Group>\n\n            <Button variant='success' type='submit' onClick={handleSubmit}>\n                Submit\n            </Button>\n            <Button variant='primary' onClick={props.toggleRegister}>\n                New User\n            </Button>\n        </Form>\n    );\n}"},"lineCount":null}},"error":null,"hash":"17caee2b146f0e4b85773ef45c223fe5","cacheData":{"env":{}}}